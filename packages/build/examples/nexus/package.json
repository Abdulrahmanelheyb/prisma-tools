{
  "name": "nexus-example",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "prisma, nexus example to create CRUD system by one command",
  "scripts": {
    "build": "nexus build",
    "start": "node node_modules/.build",
    "generate": "npm -s run generate:prisma && npm -s run generate:schema",
    "generate:prisma": "prisma generate",
    "generate:pal": "pal generate",
    "create-migration": "prisma migrate save --experimental",
    "migrate-database": "prisma migrate up --experimental --auto-approve --create-db",
    "dev": "nexus dev"
  },
  "dependencies": {
    "@prisma/client": "2.0.1",
    "nexus": "0.23.0",
    "nexus-plugin-paljs": "0.0.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.1",
    "prettier": "^2.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
