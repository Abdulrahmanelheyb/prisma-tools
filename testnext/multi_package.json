{
  "name": "testnext",
  "version": "0.0.2",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "pal g -m && yarn generate:prisma1 && yarn generate:prisma2 && yarn generate:nexus && yarn codegen",
    "generate:prisma1": "prisma generate --schema=prisma1/schema.prisma",
    "generate:prisma2": "prisma generate --schema=prisma2/schema.prisma",
    "generate:nexus": "yarn ts-node --transpile-only src/server/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "db-dev": "prisma migrate dev --schema=prisma1/schema.prisma && prisma migrate dev --schema=prisma2/schema.prisma",
    "db-deploy": "prisma migrate deploy --schema=prisma1/schema.prisma && prisma migrate deploy --schema=prisma2/schema.prisma",
    "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'"
  },
  "dependencies": {
    "@apollo/client": "3.3.19",
    "@apollo/react-ssr": "4.0.0",
    "@heroicons/react": "1.0.1",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@paljs/admin": "3.3.4",
    "@paljs/nexus": "3.3.3",
    "@prisma/client": "2.23.0",
    "apollo-server-micro": "2.24.1",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.1",
    "graphql": "15.5.0",
    "graphql-tag": "2.12.4",
    "graphql-tools": "7.0.5",
    "jsonwebtoken": "8.5.1",
    "next": "10.2.0",
    "nexus": "1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@paljs/cli": "3.3.5",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.5.0",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "15.3.0",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "autoprefixer": "10.2.5",
    "babel-plugin-graphql-tag": "3.3.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "6.0.0",
    "postcss": "8.2.15",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "prisma": "2.23.0",
