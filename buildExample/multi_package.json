  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "pal g -m && yarn generate:prisma1 && yarn generate:prisma2 && yarn generate:nexus && yarn codegen",
    "generate:prisma1": "prisma generate --schema=prisma1/schema.prisma",
    "generate:prisma2": "prisma generate --schema=prisma2/schema.prisma",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/server/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "db-dev": "prisma migrate dev --schema=prisma1/schema.prisma && prisma migrate dev --schema=prisma2/schema.prisma"
  },
  "dependencies": {
    "@apollo/client": "3.x",
